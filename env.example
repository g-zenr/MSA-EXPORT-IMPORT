# Application Configuration
NODE_ENV=production
PORT=3000
LOG_LEVEL=info

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Export Configuration
MAX_FILE_SIZE=10485760
SUPPORTED_FORMATS=csv,pdf,image
DEFAULT_DELIMITER=,
DEFAULT_QUOTE="
DEFAULT_ESCAPE="

# Memory Pool Configuration
MEMORY_POOL_SIZE=100
MEMORY_POOL_TIMEOUT=300000

# Logging
LOG_FILE_PATH=./logs
LOG_MAX_SIZE=10485760
LOG_MAX_FILES=5

# Performance
COMPRESSION_LEVEL=6
STREAMING_CHUNK_SIZE=8192 